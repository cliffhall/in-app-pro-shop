{
  "name": "in-app-pro-shop",
  "version": "1.0.0",
  "description": "An Ethereum blockchain-based system for in-app purchases",
  "private": true,
  "main": "truffle.js",
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "ganache:start": "node ./start-ganache.js",
    "app:start": "react-app-rewired start",
    "app:build": "react-app-rewired build",
    "app:build:testenv": "env-cmd .env.test npm run app:build",
    "domain:test": "npx jest --detectOpenHandles",
    "truffle:start": "npx truffle console"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cliffhall/in-app-pro-shop.git"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "in-app purchase",
    "erc721",
    "react",
    "redux",
    "javascript",
    "solidity"
  ],
  "author": "Cliff Hall",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/cliffhall/in-app-pro-shop/issues"
  },
  "homepage": "https://iaps-test.futurescale.com",
  "dependencies": {
    "bootstrap": "^3.4.1",
    "drizzle": "^1.4.0",
    "drizzle-react": "^1.2.0",
    "env-cmd": "^8.0.2",
    "@openzeppelin/contracts": "^2.4.0",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.4",
    "react-currency-masked-input": "^2.2.0",
    "react-dom": "^16.4.0",
    "react-epic-spinners": "^0.2.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.0.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.4.6",
    "web3": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "abi-decoder": "^2.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-env": "^1.7.0",
    "bn.js": "^5.0.0",
    "ganache-cli": "^6.7.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.6.2",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.1",
    "truffle": "^5.8.4",
    "truffle-assertions": "^0.9.2",
    "truffle-contract": "^4.0.31",
    "truffle-hdwallet-provider": "^1.0.14"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/index.js",
      "!src/**/*.test.js",
      "!src/index.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*Test.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
